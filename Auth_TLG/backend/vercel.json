{
  "installCommand": "npm install --legacy-peer-deps",
  "version": 2,
  "builds": [
    {
      "src": "index.js", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/users/(.*)", 
      "dest": "routes/userRoutes.js"
    },
    {
      "src": "/api/(.*)", 
      "dest": "index.js"
    }
  ]
}
